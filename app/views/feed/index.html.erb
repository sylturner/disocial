<div class="page-header">
  <h1>Feed</h1>
</div>
<table class="table table-striped">
  <tbody>
    <% @updates.each do |status| %>
      <tr>
        <td>
          <strong><%= status["friend"]["name"] %></strong><br/>
          <%= status["message"] %><br/>
          <em><%= time_ago_in_words(status["updated_at"]) %> ago</em>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
